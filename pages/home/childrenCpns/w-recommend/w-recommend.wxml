<!--pages/home/childrenCpns/w-recommend/w-recommend.wxml-->
<view class="recommend">
  <block wx:for="{{recommendList}}" wx:key="index">
    <view class="recommend-item">
      <image src="{{item.image}}" class="recommend-image" bindload="handleImageLoad"></image>
      <view class="recommend-text">{{item.title}}</view>
    </view>
  </block>
</view>